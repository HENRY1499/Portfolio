---
export interface Props {
  className?: string;
  width?: number; // Ej: '24px', '2rem', etc.
  height?: number; // Ej: '24px', '2rem', etc.
  color?: string; // Ej: '#000', 'currentColor', etc.
}

const { className = "", width = 709, height = 3 } = Astro.props;
---

<svg
  class={className}
  width={`${width}px`}
  height={`${height}px`}
  viewBox="0 0 709 3"
  fill="none"
  xmlns="http://www.w3.org/2000/svg"
>
  <line
    x1="-0.000738552"
    y1="2.5"
    x2="708.999"
    y2="1.45273"
    stroke="#9B7D61"
    stroke-width="6"></line>
</svg>
